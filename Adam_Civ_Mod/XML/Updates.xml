			<set PrereqTech="TECH_ATOMIC_THEORY"/>
			<where Type="BUILDING_MILITARY_BASE"/>
		</update>
	</Buildings>
	<Units>
		<Update>
			<set Embark="False"/>
			<where type="UNIT_HELICOPTER_GUNSHIP"/>
		</Update>
		<Update>
			<set Unhappiness="5"/>
			<where type="UNIT_NUCLEAR_MISSILE"/>
		</Update>
		<Update>
			<set Unhappiness="2"/>
			<where type="UNIT_ATOMIC_BOMB"/>
		</Update>
	</Units>
	<Building_YieldChanges>
		<update>
			<Set Yield="20"/>
			<where BuildingType="BUILDING_NATIONAL_TREASURY" YieldType="YIELD_GOLD"/>
		</update>
	</Building_YieldChanges>
</GameData>